// Computes the sum of 2/3 and 1/5.
class Main{
   function void main(){
      var Fraction a, b, c;
      let a = Fraction.new(3, 4);
      let b = Fraction.new(1, 4);
      let c = a.plus(b);
      do c.print();
      do c.dispose();
      return;
   }
}
